{
  "jsxSingleQuote": true,
  "bracketSpacing": true,
  "trailingComma": "all",
  "arrowParens": "always",
  "singleAttributePerLine": true,
  "endOfLine": "lf",
  "importOrder": [
    "<BUILTIN_MODULES>",
    "",
    "<THIRD_PARTY_MODULES>",
    "",
    "<TYPES>",
    "",
    "^@ole(.*?)",
    "",
    "^(?!.*[.]css$)[./].*$",
    "",
    ".css$"
  ],
  "importOrderParserPlugins": ["typescript", "jsx", "decorators-legacy"],
  "importOrderTypeScriptVersion": "5.2.2",
  "cssDeclarationSorterOrder": "smacss",
  "cssDeclarationSorterKeepOverrides": false,
  "printWidth": 350,
  "tabWidth": 4,
  "useTabs": false,
  "singleQuote": true,
  "semi": true,

  "htmlWhitespaceSensitivity": "ignore",
  "bracketSameLine": false,
  "overrides": [
    {
      "files": "*.html",
      "options": {
        "parser": "angular"
      }
    }
  ]
}
